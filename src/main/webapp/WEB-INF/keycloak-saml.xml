<keycloak-saml-adapter>
    <SP entityID="kiuwan-sso-saml"
        sslPolicy="EXTERNAL"
        logoutPage="error.jsp">
        <Keys>
            <Key signing="true">
                <PrivateKeyPem>
                    
                </PrivateKeyPem>
                <CertificatePem>
                    
                </CertificatePem>
            </Key>
        </Keys>

		<RoleIdentifiers>
		     <Attribute name="Role"/>
		     <Attribute name="memberOf"/>
		</RoleIdentifiers>

        <IDP entityID="idp"
             signatureAlgorithm="RSA_SHA256"
             signatureCanonicalizationMethod="http://www.w3.org/2001/10/xml-exc-c14n#">
            <SingleSignOnService signRequest="true"
                                 validateResponseSignature="true"
                                 validateAssertionSignature="false"
                                 requestBinding="POST"
                                 bindingUrl="http://localhost:8280/auth/realms/demo/protocol/saml"/>
            <SingleLogoutService signRequest="true"
                                 signResponse="true"
                                 validateRequestSignature="true"
                                 validateResponseSignature="true"
                                 requestBinding="POST"
                                 responseBinding="POST"
                                 postBindingUrl="http://localhost:8280/auth/realms/demo/protocol/saml"
                                 redirectBindingUrl="http://localhost:8280/auth/realms/demo/protocol/saml"/>
            <Keys>
                <Key signing="true">
                    <CertificatePem>
                       
                    </CertificatePem>
                </Key>
            </Keys>
        </IDP>
    </SP>
</keycloak-saml-adapter>
